    "@types/cors": "^2.8.12",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.13",
    "@types/graphql": "^14.5.0",
    "@types/knex": "^0.16.1",
    "@types/lodash.merge": "^4.6.6",
